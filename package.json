{
  "name": "spectacle-cli",
  "version": "0.3.0",
  "description": "CLI for the Spectacle Presentation Framework",
  "engines": {
    "node": ">=10"
  },
  "bin": {
    "spectacle": "./bin/spectacle/cli.js",
    "spectacle-cli": "./bin/spectacle/cli.js",
    "spectacle-boilerplate": "./bin/boilerplate/cli.js"
  },
  "scripts": {
    "preversion": "yarn run check",
    "clean:dist": "rimraf \"examples/*/dist\"",
    "clean:boilerplate": "rimraf .examples-boilerplate",
    "clean": "yarn clean:dist && yarn clean:boilerplate",
    "boilerplate:generate:js": "echo 'TODO_BP: Implement js boilerplate mode'",
    "boilerplate:generate:md": "node bin/boilerplate/cli.js -m md -d \".examples-boilerplate/cli-md\"",
    "boilerplate:generate:mdx": "node bin/boilerplate/cli.js -m mdx -d \".examples-boilerplate/cli-mdx\"",
    "boilerplate:generate": "builder concurrent --buffer boilerplate:generate:js boilerplate:generate:md boilerplate:generate:mdx",
    "boilerplate:install": "echo TODO_BP_INSTALL",
    "boilerplate:build": "echo TODO_BP_BUILD",
    "examples:build:cli-md": "cd examples/cli-md && node ../../bin/spectacle/cli.js --title \"Spectacle CLI + MD Example\" --src slides.md -a build",
    "examples:build:cli-mdx": "cd examples/cli-mdx && node ../../bin/spectacle/cli.js -l \"Spectacle CLI + MDX + Babel + Custom Theme Example\" -t theme.js -f true --action build --output dist",
    "examples:build:cli-mdx-babel": "cd examples/cli-mdx-babel && node ../../bin/spectacle/cli.js -l \"Spectacle CLI + MDX + Babel + Spectacle Slide Example\" -q template.js --action build",
    "examples:build": "builder concurrent --buffer examples:build:cli-md examples:build:cli-mdx examples:build:cli-mdx-babel",
    "start:cli-md": "cd examples/cli-md && node ../../bin/spectacle/cli.js --title \"Spectacle CLI + MD Example\" --src slides.md --port 3100",
    "start:cli-mdx": "cd examples/cli-mdx && node ../../bin/spectacle/cli.js -l \"Spectacle CLI + MDX + Custom Theme Example\" -t theme.js -f true -p 3000",
    "start:cli-mdx-babel": "cd examples/cli-mdx-babel && node ../../bin/spectacle/cli.js -l \"Spectacle CLI + MDX + Babel + Spectacle Slide Example\" -q template.js -p 3001",
    "start:examples": "builder concurrent --log-level=info start:cli-md start:cli-mdx start:cli-mdx-babel",
    "start": "yarn start:cli-mdx",
    "lint": "eslint .",
    "check": "yarn lint",
    "check:ci": "yarn run check"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/FormidableLabs/spectacle-cli.git"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.2",
    "babel-plugin-codegen": "^3.1.0",
    "builder": "^5.0.0",
    "eslint": "^6.7.2",
    "eslint-config-formidable": "^4.0.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-filenames": "^1.2.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.17.0",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.0"
  },
  "dependencies": {
    "@babel/core": "^7.7.7",
    "@babel/preset-react": "^7.7.4",
    "@mdx-js/react": "^1.5.2",
    "babel-loader": "^8.0.6",
    "file-loader": "^5.0.2",
    "fs-extra": "^8.1.0",
    "html-webpack-plugin": "^3.2.0",
    "prop-types": "^15.7.2",
    "raw-loader": "^4.0.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "spectacle": "6.0.0-alpha.3",
    "spectacle-mdx-loader": "FormidableLabs/spectacle-mdx-loader",
    "validate-npm-package-name": "^3.0.0",
    "webpack": "^4.41.4",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1"
  }
}
