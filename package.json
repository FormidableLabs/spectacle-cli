{
  "name": "spectacle-cli",
  "version": "0.0.1",
  "description": "CLI for the Spectacle Presentation Framework",
  "bin": {
    "spectacle": "./bin/cli.js",
    "spectacle-cli": "./bin/cli.js"
  },
  "scripts": {
    "preversion": "yarn run check",
    "build-examples-cli-md": "echo TODO_BUILD_CLI",
    "build-examples-cli-mdx-babel": "echo TODO_BUILD_CLI",
    "build-examples-loader-mdx": "webpack --config examples/loader-mdx/webpack.config.js",
    "build-examples": "yarn build-examples-cli-mdx-babel && yarn build-examples-loader-mdx",
    "start:cli-md": "cd examples/cli-md && node ../../bin/cli.js -l \"Spectacle CLI + MD Example\" --src slides.md --port 3001",
    "start:cli-mdx-babel": "cd examples/cli-mdx-babel && node ../../bin/cli.js -l \"Spectacle CLI + MDX + Babel + Custom Theme Example\" --port 3000",
    "start:loader-mdx": "webpack-dev-server --port=4000 --hot --config examples/loader-mdx/webpack.config.js",
    "start": "yarn start:cli-mdx-babel",
    "lint": "eslint .",
    "check": "yarn lint",
    "check-ci": "yarn run check"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/FormidableLabs/spectacle-cli.git"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.6.3",
    "babel-eslint": "^10.0.2",
    "eslint": "^5.16.0",
    "eslint-config-formidable": "^3.0.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-filenames": "^1.2.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "prettier": "^1.18.2"
  },
  "dependencies": {
    "@babel/core": "^7.6.4",
    "@mdx-js/mdx": "^1.5.1",
    "@mdx-js/react": "^1.5.1",
    "babel-loader": "^8.0.6",
    "file-loader": "^4.2.0",
    "gray-matter": "^4.0.2",
    "html-webpack-plugin": "^3.2.0",
    "normalize-newline": "^3.0.0",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0"
  }
}
