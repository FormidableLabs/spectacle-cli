{
  "name": "spectacle-cli",
  "version": "0.0.1",
  "description": "CLI for the Spectacle Presentation Framework",
  "main": "lib/index.js",
  "bin": {
    "spectacle": "./bin/cli.js",
    "spectacle-cli": "./bin/cli.js"
  },
  "scripts": {
    "preversion": "yarn run check",
    "build-examples-mdx": "webpack --config examples/mdx/webpack.config.js",
    "build": "yarn build-examples-mdx",
    "start:mdx": "webpack-dev-server --port=3000 --hot --config examples/mdx/webpack.config.js",
    "lint": "eslint --ignore-path .gitignore .",
    "check": "yarn lint"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/FormidableLabs/spectacle-cli.git"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.6.3",
    "babel-eslint": "^10.0.2",
    "eslint": "^5.16.0",
    "eslint-config-formidable": "^3.0.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-filenames": "^1.2.0",
    "eslint-plugin-import": "^2.9.0"
  },
  "dependencies": {
    "@babel/core": "^7.6.4",
    "@mdx-js/mdx": "^1.5.1",
    "@mdx-js/react": "^1.5.1",
    "babel-loader": "^8.0.6",
    "file-loader": "^4.2.0",
    "gray-matter": "^4.0.2",
    "html-webpack-plugin": "^3.2.0",
    "normalize-newline": "^3.0.0",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0"
  }
}
